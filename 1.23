package main

import "fmt"

func main() {
	// Примерный слайс
	slice := []int{10, 20, 30, 40, 50}
	fmt.Println("До удаления:", slice)

	// Индекс элемента, который нужно удалить
	i := 2 // удалим элемент с индексом 2 (значение 30)

	// Сдвигаем хвост слайса влево на одну позицию
	copy(slice[i:], slice[i+1:])

	// Уменьшаем длину слайса на 1
	slice = slice[:len(slice)-1]

	fmt.Println("После удаления:", slice)
}
